a = {\n          'type': 'datasourceEditor',\n          'id': id,\n          'width': 50\n        }\n        this.sidebars = [data]\n        this.stopMainScroll()\n        this.$emit('passSidebars', this.sidebars)\n        this.$emit('showSidebar', data)\n      } else {\n        this.restoreMainScroll()\n      }\n    }\n  },\n  mounted() {\n    this.datasources = fakeData.dataSources\n    this.checkForEditor()\n  },\n  watch: {\n    $route() {\n      this.checkForEditor()\n    }\n  }\n}\n</script>\n\n<style scoped>\ntable {\n  text-align: left;\n}\n</style>\n"],"sourceRoot":"src/components/dashboard/config/rules"}]}